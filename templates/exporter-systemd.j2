# {{ ansible_managed }}

[Unit]
Description=Prometheus Node Exporter
After=network.target

[Service]
User={{ prometheus_node_exporter_user }}
Group={{ prometheus_node_exporter_group }}
Type=simple
ExecStart={{ prometheus_node_exporter_home }}/{{ prometheus_node_exporter_process_name }} --web.listen-address=:{{ prometheus_node_exporter_port }}

[Install]
WantedBy=multi-user.target
